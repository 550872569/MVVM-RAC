#define NSSTRING_VALUE_BYKEY(DICT,KEY) [DICT ObjectByKey_NSString:KEY]
#define NSDICTIONARY_VALUE_BYKEY(DICT,KEY) [DICT ObjectByKey_NSDictionary:KEY]
