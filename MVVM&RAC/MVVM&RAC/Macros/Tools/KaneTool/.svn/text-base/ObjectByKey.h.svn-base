#define ID_VALUE_BYKEY(DICT,KEY) [DICT ObjectByKey_Id:KEY]

#define NSINTEGER_VALUE_BYKEY(DICT,KEY) [DICT ObjectByKey_NSInteger:KEY]
#define CGFLOAT_VALUE_BYKEY(DICT,KEY) [DICT ObjectByKey_CGFloat:KEY]
#define DOUBLE_VALUE_BYKEY(DICT,KEY) [DICT ObjectByKey_Double:KEY]
